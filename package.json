{
  "name": "@foodello/intercom",
  "version": "4.2.2",
  "description": "Enable Intercom features for Capacitor apps",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "all-contributors": "all-contributors",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "commitlint": "commitlint --edit",
    "contributors:add": "all-contributors add",
    "contributors:gen": "all-contributors generate",
    "docgen": "docgen --api IntercomPlugin --output-readme README.md --output-json dist/docs.json",
    "eslint": "eslint . --ext ts",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- autocorrect --format",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "prepublishOnly": "npm run build",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "prepare": "husky install",
    "swiftlint": "node-swiftlint",
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:web": "npm run build",
    "watch": "tsc --watch"
  },
  "author": "Stewan Silva",
  "license": "MIT",
  "peerDependencies": {
    "@capacitor/core": "^4.0.0"
  },
  "devDependencies": {
    "@capacitor/android": "^4.0.0",
    "@capacitor/cli": "^4.7.3",
    "@capacitor/core": "^4.0.0",
    "@capacitor/docgen": "^0.0.10",
    "@capacitor/ios": "^4.0.0",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "@types/intercom-web": "^2.8.20",
    "@types/node": "^18.16.0",
    "all-contributors-cli": "^6.20.0",
    "eslint": "^7.11.0",
    "husky": "^8.0.0",
    "prettier": "~2.3.0",
    "prettier-plugin-java": "~1.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "swiftlint": "^1.0.1",
    "typescript": "^4.9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "FoodelloIntercom.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fiksuruoka-fi/intercom"
  },
  "bugs": {
    "url": "https://github.com/fiksuruoka-fi/intercom/issues"
  }
}
